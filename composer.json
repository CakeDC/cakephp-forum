{
    "name": "cakedc/cakephp-forum",
    "description": "Forum Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "forum", "cakedc"],
    "homepage": "https://github.com/CakeDC/cakephp-forum",
    "license": "MIT",
    "authors": [
        {
            "name": "CakeDC",
            "homepage": "http://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/cakephp-forum/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/cakephp-forum/issues",
        "source": "https://github.com/CakeDC/cakephp-forum"
    },
    "require": {
        "cakephp/cakephp": "4.x-dev as 4.0.0",
        "muffin/slug": "dev-cake-4.x",
        "muffin/orderly": "dev-cake-4.x",
        "cakephp/migrations": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Forum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Forum\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "minimum-stability": "dev"
}
